<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Include shared preferences -->
        <include domain="sharedpref" path="."/>
        
        <!-- Include database -->
        <include domain="database" path="."/>
    </cloud-backup>
    
    <device-transfer>
        <!-- Include shared preferences -->
        <include domain="sharedpref" path="."/>
        
        <!-- Include database -->
        <include domain="database" path="."/>
    </device-transfer>
</data-extraction-rules>